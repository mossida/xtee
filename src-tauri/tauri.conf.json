{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "xtee",
	"version": "0.1.0",
	"identifier": "com.xtee.app",
	"build": {
		"beforeDevCommand": "bun run dev",
		"devUrl": "http://localhost:3000",
		"beforeBuildCommand": "bun run build",
		"frontendDist": "../out"
	},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"title": "xtee",
				"resizable": false,
				"fullscreen": true,
				"minimizable": false,
				"decorations": false,
				"width": 1024,
				"height": 600,
				"browserExtensionsEnabled": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": false
	}
}
